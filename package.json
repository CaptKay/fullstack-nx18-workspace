  "scripts": {
    "db:seed": "node libs/db/seed.cjs",
    "mobile:start": "cd apps/mobile-expo && expo start",
    "web:dev": "nx serve web-react",
    "api:dev": "nx serve api-express",
    "api:docker:build": "docker build -t fullstack-api-express .",
    "api:docker:run": "docker run --name fullstack-api-express-container -p 3000:3000 fullstack-api-express",
    "api:docker:stop": "docker stop fullstack-api-express-container && docker rm fullstack-api-express-container",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --target=test --all",
    "desktop:dev": "concurrently -k \"nx serve desktop-electron\" \"wait-on http://localhost:4300 && electron apps/desktop-electron/electron-main.cjs\"",
    "desktop:build": "nx build desktop-electron",
    "desktop:prod": "npm run desktop:build && concurrently -k \"npx serve -s dist/apps/desktop-electron -l 4300\" \"wait-on http://localhost:4300 && cross-env NODE_ENV=production electron apps/desktop-electron/electron-main.cjs\""
  },
